<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
   xmlns:android="http://schemas.android.com/apk/res/android"
   android:id="@+id/tab2"
   android:layout_width="fill_parent"
   android:layout_height="fill_parent"
   android:orientation="vertical">
    <FrameLayout 
    	android:layout_height="wrap_content" 
    	android:layout_width="match_parent" 
    	android:id="@+id/frameLayout1" 
    	android:layout_marginLeft="@dimen/margin_left_s" 
    	android:layout_marginRight="@dimen/margin_left_s" 
    	android:layout_weight="3">
        <LinearLayout 
        	android:layout_height="wrap_content"  
        	android:layout_width="match_parent" 
        	android:orientation="vertical" 
        	android:layout_gravity="bottom">
            <TextView 
            	android:layout_width="match_parent" 
            	android:id="@+id/pro_name" 
            	android:layout_marginLeft="7sp"
            	android:textSize="@dimen/key_word" 
            	android:layout_height="wrap_content" 
            	android:textColor="@color/key_word_color" 
            	android:text="@string/pro_adv_sear_keyword"/>
            <EditText android:id="@+id/edit_name" 
            	android:layout_width="fill_parent" 
            	android:layout_height="wrap_content" 
            	android:textSize="@dimen/text_word" 
            	android:background="@drawable/edittext"
            	android:layout_gravity="center_vertical" 
            	android:singleLine="true"/>
        </LinearLayout>
    </FrameLayout>
    <LinearLayout 
	     	android:layout_height="wrap_content" 
	     	android:orientation="vertical"
	     	android:layout_marginTop="5sp"
	     	android:layout_width="fill_parent" 
	     	android:layout_marginLeft="@dimen/margin_left_s" 
	     	android:layout_marginRight="@dimen/margin_left_s">
	        <TextView 
	        	android:layout_width="match_parent" 
	        	android:textSize="@dimen/key_word" 
	        	android:id="@+id/TextView01" 
	        	android:layout_height="wrap_content" 
	        	android:layout_marginLeft="7sp"
	        	android:textColor="@color/key_word_color" 
	        	android:text="@string/pro_adv_sear_brand"/>
	        <Spinner 
	        	android:id="@+id/sp_brand" 
	        	android:background="@drawable/spinner_style"
	        	android:layout_height="wrap_content" 
	        	android:layout_width="match_parent"/>
	    </LinearLayout>
     <LinearLayout
	     android:layout_width="fill_parent"
	     android:layout_height="wrap_content"
	     android:layout_marginTop="5sp"
	     android:layout_marginLeft="@dimen/margin_left_s" 
	     android:layout_marginRight="@dimen/margin_left_s"     
	     android:orientation="vertical">
	     <TextView
		     android:id="@+id/pro_category"
		     android:layout_height="wrap_content"
		     android:textSize="@dimen/key_word"
		     android:layout_width="match_parent" 
             android:layout_marginLeft="7sp"
		     android:textColor="@color/key_word_color" 
		     android:text="@string/pro_adv_sear_category"/>
	     <Spinner
		     android:id="@+id/sp_category"
		     android:background="@drawable/spinner_style"
		     android:layout_width="match_parent"
		     android:layout_height="wrap_content"/>
	    </LinearLayout>
	    
        <LinearLayout
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginTop="5sp"    
		    android:layout_marginLeft="@dimen/margin_left_s" 
		    android:layout_marginRight="@dimen/margin_left_s" 
		    android:orientation="vertical">
		    <TextView
			    android:id="@+id/pro_manuf"
			    android:layout_height="wrap_content" 
			    android:textSize="@dimen/key_word"
			    android:layout_marginLeft="7sp"
			    android:layout_width="match_parent" 
			    android:textColor="@color/key_word_color" 
			    android:text="@string/pro_adv_sear_manu">
	     </TextView>
	     <Spinner
		     android:id="@+id/sp_manufaturer"
		     android:background="@drawable/spinner_style"
		     android:layout_width="match_parent"
		     android:layout_height="wrap_content"/>
    </LinearLayout>
     
    <LinearLayout
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="8"
	    android:gravity="center">
        <Button android:layout_width="@dimen/btn_width" android:text="@string/gen_btn_reset" android:textColor="@color/btn_color" android:background="@drawable/btn_default" android:layout_height="@dimen/btn_height" android:layout_gravity="center" android:id="@+id/reset" android:textSize="@dimen/btn_word"></Button>
        <Button android:layout_width="@dimen/btn_width" 
	        android:layout_height="@dimen/btn_height" 
	        android:text="@string/b1_search" 
	        android:textSize="@dimen/btn_word" 
	        android:layout_marginLeft="20sp"
	        android:id="@+id/Adv_Search" 
	        android:layout_gravity="center" 
	        android:background="@drawable/btn_default" 
	        android:textColor="@color/btn_color"/>
    </LinearLayout>
</LinearLayout>