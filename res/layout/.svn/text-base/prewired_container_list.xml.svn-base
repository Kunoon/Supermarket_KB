<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent" 
	android:id="@id/gen_background">
    <LinearLayout 
    	android:layout_height="@dimen/title_height" 
    	android:layout_width="match_parent" 
    	android:background="@drawable/titlebar" 
    	android:id="@+id/textView1">
        <Button 
        	android:layout_width="@dimen/title_btn_width" 
        	android:textColor="@color/btn_color" 
        	android:id="@id/gen_btn_back" 
        	android:background="@drawable/title_btn_back" 
        	android:layout_height="wrap_content" 
        	android:textSize="@dimen/title_btn_word"
        	android:layout_gravity="center_vertical" 
        	android:text="@string/gen_btn_back"/>
        <TextView 
        	android:layout_width="wrap_content" 
        	android:layout_weight="1" 
        	android:gravity="center" 
        	android:textSize="@dimen/title_word"
        	android:textColor="@color/title_color"
        	android:id="@+id/textView3" 
        	android:layout_height="wrap_content" 
        	android:layout_gravity="center" 
        	android:text="@string/temp_list_tag"/>
        <Button 
        	android:layout_width="@dimen/title_btn_width" 
        	android:textColor="@color/btn_color" 
        	android:id="@id/gen_btn_main" 
        	android:textSize="@dimen/title_btn_word"
        	android:background="@drawable/title_btn_main" 
        	android:layout_height="wrap_content" 
        	android:layout_gravity="center_vertical" 
        	android:text="@string/gen_btn_main"/>
    </LinearLayout>
    <TableLayout 
    	android:layout_height="wrap_content" 
    	android:layout_width="match_parent" 
    	android:id="@+id/tableLayout1" 
    	android:layout_marginLeft="10sp"
    	android:layout_marginRight="10sp"
    	android:layout_marginBottom="10sp"
    	android:layout_marginTop="20sp">
        <TableRow 
        	android:id="@+id/tableRow1" 
        	android:layout_width="match_parent" 
        	android:layout_height="wrap_content">
            <TextView
	     		android:id="@+id/tag"
	     		android:layout_marginLeft="5sp"
	     		android:textSize="@dimen/key_word"
	     		android:textColor="@color/key_word_color"
	    		android:layout_height="wrap_content"
	     		android:layout_gravity="center_vertical"
	    		android:layout_width="wrap_content" 
	    		android:layout_marginRight="20sp" 
	    		android:text="@string/temp_list_port"/>
    		<Spinner
	    		android:layout_width="match_parent"
	     		android:layout_height="wrap_content"     
	     		android:layout_gravity="center_vertical" 
	     		android:layout_marginRight="10sp" 
	     		android:id="@+id/sp_port" 
	     		android:background="@drawable/spinner_style"
	     		android:layout_weight="1"/>
        </TableRow>
        <TableRow 
        	android:id="@+id/tableRow2" 
        	android:layout_marginTop="15sp"
        	android:layout_width="wrap_content" 
        	android:layout_height="wrap_content">
            <TextView 
            	android:id="@+id/TextView01" 
            	android:layout_height="wrap_content" 
            	android:layout_gravity="center_vertical"
	     		android:layout_marginLeft="5sp"
	     		android:layout_marginRight="20sp" 
	     		android:textSize="@dimen/key_word"
	     		android:textColor="@color/key_word_color"
            	android:layout_width="wrap_content" 
            	android:text="@string/temp_list_zone"/>
            <Spinner 
            	android:layout_weight="1" 
            	android:layout_height="wrap_content" 
            	android:id="@+id/sp_zone" 
            	android:background="@drawable/spinner_style"
            	android:layout_width="fill_parent" 
            	android:layout_gravity="center_vertical" 
            	android:layout_marginRight="10sp"/>
        </TableRow>
        <TableRow 
        	android:id="@+id/tableRow3" 
        	android:layout_marginTop="15sp"
        	android:layout_width="wrap_content" 
        	android:layout_height="wrap_content">
            <TextView 
            	android:id="@+id/TextView02" 
            	android:layout_height="wrap_content" 
            	android:layout_gravity="center_vertical" 
	     		android:layout_marginLeft="5sp"
	     		android:layout_marginRight="20sp" 
	     		android:textSize="@dimen/key_word"
	     		android:textColor="@color/key_word_color"
            	android:layout_width="wrap_content" 
            	android:text="@string/temp_list_brand"/>
            <Spinner 
            	android:layout_weight="1" 
            	android:layout_height="wrap_content" 
            	android:id="@+id/sp_brand" 
            	android:background="@drawable/spinner_style"
            	android:layout_width="fill_parent" 
            	android:layout_gravity="center_vertical" 
            	android:layout_marginRight="10sp"/>
        </TableRow>
    </TableLayout>
    <ListView
	    android:id="@+id/lv_serachresult"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" 
	    android:cacheColorHint="#00000000"
	    android:layout_weight="1" 
	    android:layout_marginLeft="15sp" 
	    android:layout_marginBottom="5sp" 
	    android:layout_marginRight="15sp" 
	    android:layout_marginTop="15sp"
	    android:divider="@color/list_line"    
	    android:listSelector="@drawable/list_selector"   
		android:dividerHeight="0.5dp"/>
    <FrameLayout android:id="@+id/FrameLayout1"
	    android:orientation="horizontal"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content" 
	    android:layout_marginBottom="10dp">
   		<Button
	    	android:id="@+id/refresh"
	    	android:layout_width="110sp"
	    	android:layout_height="@dimen/btn_height"
	    	android:textSize="@dimen/btn_word"
	    	android:layout_gravity="center" 
	    	android:background="@drawable/btn_default" 
	    	android:text="@string/gen_btn_refresh" 
	    	android:textColor="@color/btn_color"/>
    </FrameLayout>
    <TextView 
    	android:layout_width="match_parent" 
    	android:id="@+id/textView2" 
    	android:background="@drawable/bottom" 
    	android:layout_height="@dimen/bottom_bar_height"/>
</LinearLayout>
